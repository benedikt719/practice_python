import time


def calculate_2(number):

    print(sum(list([x for x in range(number) if x % 3 == 0 or x % 5 == 0])))



def calculate(number):

    result = 0
    i = 1
    while i < number:

        if i % 3 == 0 or i % 5 == 0 :
            result += i
        i += 1

    print(result)


start_time = time.time()

calculate_2(1000)

end_time = time.time()


print(end_time - start_time)


calculate_2(1000)